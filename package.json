{
  "name": "garursaili-css",
  "version": "1.0.2",
  "private": true,
  "main": "dist/cli.cjs",
  "type": "module",
  "module": "dist/garur.esm.js",
  "bin": {
    "garur": "./dist/cli.cjs"
  },
  "scripts": {
    "saili:build": "tsx src/cli.ts",
    "saili:watch": "node ./dist/cli-watcher.js --watch",
    "dev": "vite",
    "start": "npm run saili:build && npm run dev",
    "build": "tsc && rollup -c --bundleConfigAsCjs && chmod +x dist/cli.cjs",
    "test": "node dist/cli.js --help",
    "bundle": "rollup -c",
    "jit": "node dist/cli.js",
    "generate-shortmap": "ts-node generate-shortmap.ts",
    "docs:gen": "ts-node src/scripts/generate-docs.ts",
    "purge": "ts-node src/scripts/purge.ts",
    "test:ci": "ts-node src/tests/run-ci-tests.ts",
    "test:jest": "jest --runInBand",
    "devV": "vite",
    "preview": "vite preview"
  },
  "keywords": [
    "garur-css",
    "saili-css",
    "garursaili",
    "barshan",
    "framework",
    "css",
    "atomic",
    "jit",
    "ui",
    "components",
    "glassmorphism",
    "dark-mode"
  ],
  "author": "Barshan Sarkar",
  "license": "MIT",
  "description": "The most beautiful atomic CSS framework .",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/chokidar": "^2.1.7",
    "@types/jest": "^29.5.0",
    "@types/minimist": "1.2.2",
    "chokidar": "^5.0.0",
    "fast-glob": "^3.3.2",
    "jest": "^29.5.0",
    "minimist": "^1.2.8",
    "rollup": "^4.53.3",
    "rollup-plugin-strip-shebang": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "cli-spinners": "^3.3.0",
    "css.escape": "^1.5.1",
    "express": "^5.2.1",
    "figlet": "^1.9.4",
    "fs-extra": "^11.3.2",
    "gradient-string": "^3.0.0",
    "log-symbols": "^7.0.1",
    "ora": "^9.0.0",
    "picocolors": "^1.1.1"
  }
}
